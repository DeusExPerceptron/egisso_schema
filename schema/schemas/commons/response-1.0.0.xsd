<?xml version="1.0" encoding="utf-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:tns="http://roskazna.ru/gisec/xsd/response/1.0.0" xmlns:gisec="http://roskazna.ru/gisec/xsd/commons/1.0.0" targetNamespace="http://roskazna.ru/gisec/xsd/response/1.0.0" elementFormDefault="qualified" attributeFormDefault="unqualified">
    <xsd:annotation>
        <xsd:documentation xml:lang="ru">Тип данных: ответ на запрос вида сведений</xsd:documentation>
    </xsd:annotation>
    <xsd:import namespace="http://roskazna.ru/gisec/xsd/commons/1.0.0" schemaLocation="gisec-commons-1.0.0.xsd"/>
    
    <xsd:complexType name="responseData">
        <xsd:annotation>
            <xsd:documentation xml:lang="ru">Тип данных: содержимое ответа на запрос вида сведений</xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
        	
            <xsd:element name="receivingTime" type="gisec:timestamp">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Дата и время приема сообщения</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="processingTime" type="gisec:timestamp">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Дата и время обработки сообщения</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="isProcessedSuccessfully" type="xsd:boolean">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Признак успешной обработки запроса</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element name="processingProtocol" minOccurs="0">
                <xsd:annotation>
                    <xsd:documentation xml:lang="ru">Протокол обработки запроса</xsd:documentation>
                </xsd:annotation>
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="message" maxOccurs="unbounded">
                            <xsd:annotation>
                                <xsd:documentation xml:lang="ru">Сообщение об ошибке или предупреждении</xsd:documentation>
                            </xsd:annotation>
                            <xsd:complexType>
                                <xsd:sequence>
                                    <xsd:element name="messageType">
                                        <xsd:annotation>
                                            <xsd:documentation xml:lang="ru">Тип сообщения</xsd:documentation>
                                        </xsd:annotation>
                                        <xsd:simpleType>
                                            <xsd:restriction base="xsd:positiveInteger">
                                                <xsd:enumeration value="1">
                                                    <xsd:annotation>
                                                        <xsd:documentation xml:lang="ru">Ошибка</xsd:documentation>
                                                    </xsd:annotation>
                                                </xsd:enumeration>
                                                <xsd:enumeration value="2">
                                                    <xsd:annotation>
                                                        <xsd:documentation xml:lang="ru">Предупреждение</xsd:documentation>
                                                    </xsd:annotation>
                                                </xsd:enumeration>
                                            </xsd:restriction>
                                        </xsd:simpleType>
                                    </xsd:element>
                                    <xsd:element name="ruleCode" type="gisec:digits-3" minOccurs="0">
                                        <xsd:annotation>
                                            <xsd:documentation xml:lang="ru">Код нарушенного правила проверки</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="messageData" type="gisec:string-500">
                                        <xsd:annotation>
                                            <xsd:documentation xml:lang="ru">Содержимое сообщения об ошибке или предупреждении</xsd:documentation>
                                        </xsd:annotation>
                                    </xsd:element>
                                </xsd:sequence>
                            </xsd:complexType>
                        </xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
</xsd:schema>