<?xml version="1.0" encoding="utf-8"?>
<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://fri.pfr.ru"
           xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:fri="http://fri.pfr.ru">

  <xs:complexType name="ActualInvalidityInfo">
    <xs:sequence>
      <xs:element type="xs:date" name="ДатаС"/>
      <xs:element type="xs:date" name="ДатаПо"/>
      <xs:element type="xs:string" name="НомерАктаМСЭ" nillable="true"/>
      <xs:element type="xs:date" name="ДатаАктаМСЭ" nillable="true"/>
      <xs:element type="xs:string" name="СерияСправкиМСЭ" nillable="true"/>
      <xs:element type="xs:string" name="НомерСправкиМСЭ" nillable="true"/>
      <xs:element type="xs:date" name="ДатаСправкиМСЭ" nillable="true"/>
      <xs:element type="xs:boolean" name="ПервичнаяИнвалидность" nillable="true"/>
      <xs:element type="xs:date" name="ДатаПервичногоОсвидетельствования" nillable="true"/>
      <xs:element type="xs:date" name="ОчередноеОсвидетельствование" nillable="true"/>
      <xs:element type="xs:integer" name="ГруппаИнвалидности"/>
      <xs:element type="xs:boolean" name="Ребенок"/>
      <xs:element type="xs:boolean" name="ИнвалидПоЗрению" nillable="true"/>
      <xs:element type="xs:long" name="КодПричиныИнвалидности" nillable="true"/>
      <xs:element type="xs:string" name="НаименованиеПричиныИнвалидности" nillable="true"/>
    </xs:sequence>
  </xs:complexType>


  <xs:complexType name="SourceType">
    <xs:sequence>
      <xs:element type="xs:string" name="Код">
        <xs:annotation>
          <xs:documentation>Идентификатор источника поступления сведений по системному справочнику ФГИС ФРИ
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element type="xs:string" name="Наименование">
        <xs:annotation>
          <xs:documentation>Наименование источника поступления сведений</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="DocType">
    <xs:sequence>
      <xs:element type="xs:string" name="Код">
        <xs:annotation>
          <xs:documentation>Код типа файла/документа по системному справочнику ФГИС ФРИ</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element type="xs:string" name="Наименование">
        <xs:annotation>
          <xs:documentation>Наименование типа файла/документа</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="PersonDataType">
    <xs:sequence>
      <xs:element name="СНИЛС">
        <xs:annotation>
          <xs:documentation>СНИЛС инвалида</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:pattern value="[0-9]{3}-[0-9]{3}-[0-9]{3}\s[0-9]{2}"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>

      <xs:element type="xs:string" name="Фамилия">
        <xs:annotation>
          <xs:documentation>Фамилия инвалида</xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element type="xs:string" name="Имя">
        <xs:annotation>
          <xs:documentation>Имя инвалида</xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element type="xs:string" name="Отчество" nillable="true">
        <xs:annotation>
          <xs:documentation>Отчество инвалида</xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element type="xs:string" name="ДатаРождения">
        <xs:annotation>
          <xs:documentation>Дата рождения инвалида</xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element name="Пол">
        <xs:annotation>
          <xs:documentation>Пол инвалида</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="F"/>
            <xs:enumeration value="M"/>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>

      <xs:element type="xs:dateTime" name="ДатаРегистрации">
        <xs:annotation>
          <xs:documentation>Дата и время регистрации инвалида в реестре</xs:documentation>
        </xs:annotation>
      </xs:element>

      <!--
      <xs:element type="fri:ActualInvalidityPeriod" name="АктуальныеСрокиИнвалидности">
	<xs:annotation>
		<xs:documentation>Актуальный период инвалидности</xs:documentation>
	</xs:annotation>
      </xs:element>
-->

      <xs:element type="fri:ActualInvalidityInfo" name="АктуальныеСведенияОбИнвалидности">
        <xs:annotation>
          <xs:documentation>Актуальный сведения об инвалидности гражданина</xs:documentation>
        </xs:annotation>
      </xs:element>

    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="FilePropertiesType">
    <xs:sequence>
      <xs:element type="xs:string" name="ID">
        <xs:annotation>
          <xs:documentation>Уникальный идентификатор файла или документа, из которого получены сведения
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element type="xs:string" name="Ключ" nillable="true">
        <xs:annotation>
          <xs:documentation>Ключ файла или документа. Ключ образуется на основании определенных полей внутри файла
            (например, номер документа, дата документа) и может использоваться для связывания сведений между собой
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element type="xs:string" name="РодительскийФайл" nillable="true">
        <xs:annotation>
          <xs:documentation>Ссылка на поле 'Ключ' родительского документа</xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element type="fri:SourceType" name="Источник">
        <xs:annotation>
          <xs:documentation>Сведения об источнике получения сведений</xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element type="xs:string" name="Номер" nillable="true">
        <xs:annotation>
          <xs:documentation>Номер файла/документа</xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element type="xs:date" name="Дата" nillable="true">
        <xs:annotation>
          <xs:documentation>Дата формирования сведений файла или дата документа</xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element type="xs:date" name="СрокДействия" nillable="true">
        <xs:annotation>
          <xs:documentation>Срок действия сведений из файла/документа</xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element type="xs:dateTime" name="ДатаРегистрации">
        <xs:annotation>
          <xs:documentation>Дата регистрации сведений из файла в реестре</xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element type="fri:DocType" name="ТипФайла">
        <xs:annotation>
          <xs:documentation>Сведения о типе файла</xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element type="xs:boolean" name="ДокументОснование">
        <xs:annotation>
          <xs:documentation>Признак, что файл является документов основания</xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element type="xs:boolean" name="ДокументИсполнение">
        <xs:annotation>
          <xs:documentation>Признак, что файл является документом исполнения</xs:documentation>
        </xs:annotation>
      </xs:element>

    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="CategoryType">
    <xs:sequence>
      <xs:element type="xs:string" name="Код">
        <xs:annotation>
          <xs:documentation>Код категории записи</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element type="xs:string" name="Домен" nillable="true">
        <xs:annotation>
          <xs:documentation>Код группы, которой принадлежит данная категория записи</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element type="xs:string" name="Наименование">
        <xs:annotation>
          <xs:documentation>Наименование категории записи</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="RecordType">
    <xs:sequence>

      <xs:element type="xs:long" name="ИдентификаторОбъекта" nillable="true">
        <xs:annotation>
          <xs:documentation>Идентификатор объекта метаданных (уникальный идентификатор экземпляра категории записи в
            конкретном типе документа)
          </xs:documentation>
        </xs:annotation>
      </xs:element>

      <xs:element type="xs:string" name="Ключ" nillable="true">
        <xs:annotation>
          <xs:documentation>Уникальный ключ для ссылки на конкретную запись в выписке</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element type="xs:string" name="Родитель" nillable="true">
        <xs:annotation>
          <xs:documentation>Ссылка на поле 'Ключ' родительской записи</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element type="fri:CategoryType" name="Категория">
        <xs:annotation>
          <xs:documentation>Сведения о категории записи</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>


  <xs:complexType name="AllRecordsType">
    <xs:sequence>
      <xs:element name="ГруппаВыплатыПФР" maxOccurs="1" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="ВыплатыПФР" maxOccurs="unbounded" minOccurs="0">
              <xs:complexType>
                <xs:all>
                  <xs:element type="fri:RecordType" name="РеквизитыЗаписи"/>
                  <xs:element name="ТипВыплат" nillable="true" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Тип выплат</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                      <xs:simpleContent>
                        <xs:extension base="xs:decimal">
                          <xs:attribute type="xs:string" name="id"/>
                          <xs:attribute type="xs:string" name="name"/>
                        </xs:extension>
                      </xs:simpleContent>
                    </xs:complexType>
                  </xs:element>
                  <xs:element name="НаименованиеТипаВыплат" nillable="true" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Наименование тип выплат по НСИ ФРИ</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                      <xs:simpleContent>
                        <xs:extension base="xs:string">
                          <xs:attribute type="xs:string" name="id"/>
                          <xs:attribute type="xs:string" name="name"/>
                        </xs:extension>
                      </xs:simpleContent>
                    </xs:complexType>
                  </xs:element>
                  <xs:element name="ВидВыплат" nillable="true" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Вид выплат</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                      <xs:simpleContent>
                        <xs:extension base="xs:decimal">
                          <xs:attribute type="xs:string" name="id"/>
                          <xs:attribute type="xs:string" name="name"/>
                        </xs:extension>
                      </xs:simpleContent>
                    </xs:complexType>
                  </xs:element>
                  <xs:element name="НаименованиеВидаВыплат" nillable="true" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Наименование вида выплат по НСИ ФРИ</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                      <xs:simpleContent>
                        <xs:extension base="xs:string">
                          <xs:attribute type="xs:string" name="id"/>
                          <xs:attribute type="xs:string" name="name"/>
                        </xs:extension>
                      </xs:simpleContent>
                    </xs:complexType>
                  </xs:element>
                  <xs:element name="СуммаВыплатыРуб" nillable="true" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Сумма выплаты (в руб)</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                      <xs:simpleContent>
                        <xs:extension base="xs:decimal">
                          <xs:attribute type="xs:string" name="id"/>
                          <xs:attribute type="xs:string" name="name"/>
                        </xs:extension>
                      </xs:simpleContent>
                    </xs:complexType>
                  </xs:element>
                  <xs:element name="ДатаНачалаВыплат" nillable="true" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Дата начала выплат</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                      <xs:simpleContent>
                        <xs:extension base="xs:date">
                          <xs:attribute type="xs:string" name="id"/>
                          <xs:attribute type="xs:string" name="name"/>
                        </xs:extension>
                      </xs:simpleContent>
                    </xs:complexType>
                  </xs:element>
                  <xs:element name="ДатаОкончанияВыплат" nillable="true" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Дата окончания выплат</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                      <xs:simpleContent>
                        <xs:extension base="xs:date">
                          <xs:attribute type="xs:string" name="id"/>
                          <xs:attribute type="xs:string" name="name"/>
                        </xs:extension>
                      </xs:simpleContent>
                    </xs:complexType>
                  </xs:element>
                  <xs:element name="КодОснованияНазначенияВыплат" nillable="true" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Код основания для назначения выплат</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                      <xs:simpleContent>
                        <xs:extension base="xs:string">
                          <xs:attribute type="xs:string" name="id"/>
                          <xs:attribute type="xs:string" name="name"/>
                        </xs:extension>
                      </xs:simpleContent>
                    </xs:complexType>
                  </xs:element>
                  <xs:element name="ПризнакУстановленияНСУ1" nillable="true" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Признак получения (отказа) НСУ-1</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                      <xs:simpleContent>
                        <xs:extension base="xs:boolean">
                          <xs:attribute type="xs:string" name="id"/>
                          <xs:attribute type="xs:string" name="name"/>
                        </xs:extension>
                      </xs:simpleContent>
                    </xs:complexType>
                  </xs:element>
                  <xs:element name="ПризнакУстановленияНСУ2" nillable="true" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Признак получения (отказа) НСУ-2</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                      <xs:simpleContent>
                        <xs:extension base="xs:boolean">
                          <xs:attribute type="xs:string" name="id"/>
                          <xs:attribute type="xs:string" name="name"/>
                        </xs:extension>
                      </xs:simpleContent>
                    </xs:complexType>
                  </xs:element>
                  <xs:element name="ПризнакУстановленияНСУ3" nillable="true" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Признак получения (отказа) НСУ-3</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                      <xs:simpleContent>
                        <xs:extension base="xs:boolean">
                          <xs:attribute type="xs:string" name="id"/>
                          <xs:attribute type="xs:string" name="name"/>
                        </xs:extension>
                      </xs:simpleContent>
                    </xs:complexType>
                  </xs:element>
                  <xs:element name="ПричинаПрекращенияВыплаты" nillable="true" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Причина приостановления/прекращения выплаты</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                      <xs:simpleContent>
                        <xs:extension base="xs:decimal">
                          <xs:attribute type="xs:string" name="id"/>
                          <xs:attribute type="xs:string" name="name"/>
                        </xs:extension>
                      </xs:simpleContent>
                    </xs:complexType>
                  </xs:element>
                  <xs:element name="НаименованиеПричиныПрекращенияВыплаты" nillable="true" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Наименование причины приостановления/прекращения выплаты</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                      <xs:simpleContent>
                        <xs:extension base="xs:string">
                          <xs:attribute type="xs:string" name="id"/>
                          <xs:attribute type="xs:string" name="name"/>
                        </xs:extension>
                      </xs:simpleContent>
                    </xs:complexType>
                  </xs:element>
                  <xs:element name="ДатаПрекращенияВыплаты" nillable="true" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Дата приостановления/прекращения выплаты</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                      <xs:simpleContent>
                        <xs:extension base="xs:date">
                          <xs:attribute type="xs:string" name="id"/>
                          <xs:attribute type="xs:string" name="name"/>
                        </xs:extension>
                      </xs:simpleContent>
                    </xs:complexType>
                  </xs:element>
                  <xs:element name="ДатаВозобновленияВыплаты" nillable="true" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Дата с которой выплата пенсии возобновлена</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                      <xs:simpleContent>
                        <xs:extension base="xs:date">
                          <xs:attribute type="xs:string" name="id"/>
                          <xs:attribute type="xs:string" name="name"/>
                        </xs:extension>
                      </xs:simpleContent>
                    </xs:complexType>
                  </xs:element>
                  <xs:element name="ДатаПервичногоУстановленияВыплаты" nillable="true" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Дата первичного установления выплаты</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                      <xs:simpleContent>
                        <xs:extension base="xs:date">
                          <xs:attribute type="xs:string" name="id"/>
                          <xs:attribute type="xs:string" name="name"/>
                        </xs:extension>
                      </xs:simpleContent>
                    </xs:complexType>
                  </xs:element>
                  <xs:element name="ДатаОбращения" nillable="true" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Дата обращения</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                      <xs:simpleContent>
                        <xs:extension base="xs:date">
                          <xs:attribute type="xs:string" name="id"/>
                          <xs:attribute type="xs:string" name="name"/>
                        </xs:extension>
                      </xs:simpleContent>
                    </xs:complexType>
                  </xs:element>
                </xs:all>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="ВсеАдреса" maxOccurs="1" minOccurs="0">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="Адрес" maxOccurs="unbounded" minOccurs="0">
              <xs:complexType>
                <xs:all>
                  <xs:element type="fri:RecordType" name="РеквизитыЗаписи"/>
                  <xs:element name="ТипАдреса" nillable="true" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Тип адреса</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                      <xs:simpleContent>
                        <xs:extension base="xs:decimal">
                          <xs:attribute type="xs:string" name="id"/>
                          <xs:attribute type="xs:string" name="name"/>
                        </xs:extension>
                      </xs:simpleContent>
                    </xs:complexType>
                  </xs:element>
                  <xs:element name="ПочтовыйИндекс" nillable="true" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Почтовый индекс</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                      <xs:simpleContent>
                        <xs:extension base="xs:string">
                          <xs:attribute type="xs:string" name="id"/>
                          <xs:attribute type="xs:string" name="name"/>
                        </xs:extension>
                      </xs:simpleContent>
                    </xs:complexType>
                  </xs:element>
                  <xs:element name="НаименованиеРегиона" nillable="true" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Наименование региона</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                      <xs:simpleContent>
                        <xs:extension base="xs:string">
                          <xs:attribute type="xs:string" name="id"/>
                          <xs:attribute type="xs:string" name="name"/>
                        </xs:extension>
                      </xs:simpleContent>
                    </xs:complexType>
                  </xs:element>
                  <xs:element name="Город" nillable="true" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Город</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                      <xs:simpleContent>
                        <xs:extension base="xs:string">
                          <xs:attribute type="xs:string" name="id"/>
                          <xs:attribute type="xs:string" name="name"/>
                        </xs:extension>
                      </xs:simpleContent>
                    </xs:complexType>
                  </xs:element>
                  <xs:element name="НаименованиеРайона" nillable="true" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Наименование района</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                      <xs:simpleContent>
                        <xs:extension base="xs:string">
                          <xs:attribute type="xs:string" name="id"/>
                          <xs:attribute type="xs:string" name="name"/>
                        </xs:extension>
                      </xs:simpleContent>
                    </xs:complexType>
                  </xs:element>
                  <xs:element name="НаименованиеУлицы" nillable="true" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Наименование улицы</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                      <xs:simpleContent>
                        <xs:extension base="xs:string">
                          <xs:attribute type="xs:string" name="id"/>
                          <xs:attribute type="xs:string" name="name"/>
                        </xs:extension>
                      </xs:simpleContent>
                    </xs:complexType>
                  </xs:element>
                  <xs:element name="НомерДома" nillable="true" minOccurs="0">
                    <xs:annotation>
                      <xs:documentation>Номер дома</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                      <xs:simpleContent>
                        <xs:extension base="xs:string">
                          <xs:attribute type="xs:string" name="id"/>
