<?xml version="1.0" encoding="utf-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:smev="urn://x-artefacts-smev-gov-ru/supplementary/commons/1.2" xmlns="urn://egisso-ru/types/basic/2.2.0" targetNamespace="urn://egisso-ru/types/basic/2.2.0" elementFormDefault="qualified" attributeFormDefault="unqualified">
  <xsd:annotation>
    <xsd:documentation xml:lang="ru">Базовые типы ЕГИССО</xsd:documentation>
  </xsd:annotation>
  <xsd:import namespace="urn://x-artefacts-smev-gov-ru/supplementary/commons/1.2" schemaLocation="../commons/smev-supplementary-commons-1.2.xsd"/>
  <!--========================================== Типы данных физического лица  ==========================================-->
  <xsd:complexType name="tPhysicalPersonWithoutBirthPlace">
    <xsd:annotation>
      <xsd:documentation xml:lang="ru">Тип физического лица со всеми обязательными данными кроме места рождения</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:restriction base="tPhysicalPersonData">
        <xsd:sequence>
          <xsd:element name="snils" type="smev:SNILSType">
            <xsd:annotation>
              <xsd:documentation xml:lang="ru">Страховой номер индивидуального лицевого счёта</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="fio" type="tFio">
            <xsd:annotation>
              <xsd:documentation xml:lang="ru">ФИО</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="gender" type="tGender">
            <xsd:annotation>
              <xsd:documentation xml:lang="ru">Пол</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="birthDate" type="tDate">
            <xsd:annotation>
              <xsd:documentation xml:lang="ru">Дата рождения</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="citizenshipCode" type="tOksmCode">
            <xsd:annotation>
              <xsd:documentation xml:lang="ru">Код страны гражданства</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:restriction>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="tPhysicalPersonWithNameAndBirthDate">
    <xsd:annotation>
      <xsd:documentation>Тип физического лица со СНИЛС, ФИО и датой рождения</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:restriction base="tPhysicalPersonData">
        <xsd:sequence>
          <xsd:element name="snils" type="smev:SNILSType">
            <xsd:annotation>
              <xsd:documentation>Страховой номер индивидуального лицевого счёта</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="fio" type="tFio">
            <xsd:annotation>
              <xsd:documentation>ФИО</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="birthDate" type="tDate">
            <xsd:annotation>
              <xsd:documentation>Дата рождения</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:restriction>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="tPhysicalPersonWithName">
    <xsd:annotation>
      <xsd:documentation>Тип физического лица со СНИЛС и ФИО</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:restriction base="tPhysicalPersonData">
        <xsd:sequence>
          <xsd:element name="snils" type="smev:SNILSType">
            <xsd:annotation>
              <xsd:documentation>Страховой номер индивидуального лицевого счёта</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="fio" type="tFio">
            <xsd:annotation>
              <xsd:documentation>ФИО</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:restriction>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="tPhysicalPersonSnilsOnly">
    <xsd:annotation>
      <xsd:documentation xml:lang="ru">Тип физического лица только со СНИЛС</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:restriction base="tPhysicalPersonData">
        <xsd:sequence>
          <xsd:element name="snils" type="smev:SNILSType">
            <xsd:annotation>
              <xsd:documentation xml:lang="ru">Страховой номер индивидуального лицевого счёта</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:restriction>
    </xsd:complexContent>
  </xsd:complexType>
  <!--========================================== Типы, используемые в данных физического лица  ==========================================-->
  <xsd:complexType name="tPhysicalPersonData">
    <xsd:annotation>
      <xsd:documentation xml:lang="ru">Данные, используемые в типах физического лица</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="snils" type="smev:SNILSType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation xml:lang="ru">Страховой номер индивидуального лицевого счёта</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="fio" type="tFio" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation xml:lang="ru">ФИО</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="gender" type="tGender" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation xml:lang="ru">Пол</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="birthDate" type="tDate" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation xml:lang="ru">Дата рождения</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="birthPlace" type="tAddress" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation xml:lang="ru">Место рождения</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="citizenshipCode" type="tOksmCode" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation xml:lang="ru">Код страны гражданства</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="tFio">
    <xsd:annotation>
      <xsd:documentation xml:lang="ru">Фамилия, имя и отчество</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="surname" type="string-100">
        <xsd:annotation>
          <xsd:documentation xml:lang="ru">Фамилия</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="patronymic" type="string-100" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation xml:lang="ru">Отчество</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="forename" type="string-100">
        <xsd:annotation>
          <xsd:documentation xml:lang="ru">Имя</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:simpleType name="tGender">
    <xsd:annotation>
      <xsd:documentation xml:lang="ru">Пол</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="string-1">
      <xsd:pattern value="[MFU]"/>
      <xsd:enumeration value="M">
        <xsd:annotation>
          <xsd:documentation xml:lang="ru">Мужской пол</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="F">
        <xsd:annotation>
          <xsd:documentation xml:lang="ru">Женский пол</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="U">
        <xsd:annotation>
          <xsd:documentation xml:lang="ru">Пол не определен</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="tDefinedGender">
    <xsd:annotation>
      <xsd:documentation xml:lang="ru">Идентифицированный пол</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="string-1">
      <xsd:pattern value="[MF]"/>
      <xsd:enumeration value="M">
        <xsd:annotation>
          <xsd:documentation xml:lang="ru">Мужской пол</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
      <xsd:enumeration value="F">
        <xsd:annotation>
          <xsd:documentation xml:lang="ru">Женский пол</xsd:documentation>
        </xsd:annotation>
      </xsd:enumeration>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="tSnils-9">
    <xsd:annotation>
      <xsd:documentation xml:lang="ru">СНИЛС (9-ти разрядный) без контрольной суммы</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:length value="9"/>
      <xsd:pattern value="\d{9}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <!--========================================== Типы данных юридического лица  ==========================================-->
  <xsd:complexType name="tLegalPersonOrSoleProprietor">
    <xsd:annotation>
      <xsd:documentation xml:lang="ru">Тип юридического лица (ЮЛ) или индивидуального предпринимателя (ИП) со всеми обязательными данными</xsd:documentation>
    </xsd:annotation>
    <xsd:choice>
      <xsd:element name="legalPerson" type="tLegalPerson">
        <xsd:annotation>
          <xsd:documentation xml:lang="ru">Юридическое лицо (ЮЛ)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="soleProprietor" type="tSoleProprietor">
        <xsd:annotation>
          <xsd:documentation xml:lang="ru">Индивидуальный предприниматель (ИП)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:choice>
  </xsd:complexType>
  <xsd:complexType name="tLegalPerson">
    <xsd:annotation>
      <xsd:documentation xml:lang="ru">Тип юридического лица со всеми обязательными данными</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:restriction base="tLegalPersonData">
        <xsd:sequence>
          <xsd:element name="name" type="string-1000">
            <xsd:annotation>
              <xsd:documentation xml:lang="ru">Наименование</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="inn" type="smev:LegalPersonINNType">
            <xsd:annotation>
              <xsd:documentation xml:lang="ru">ИНН</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="kpp" type="smev:KPPType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation xml:lang="ru">КПП</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="ogrn" type="smev:OGRNCompanyType">
            <xsd:annotation>
              <xsd:documentation xml:lang="ru">ОГРН</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:restriction>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="tSoleProprietor">
    <xsd:annotation>
      <xsd:documentation xml:lang="ru">Тип индивидуального предпринимателя (ИП) со всеми обязательными данными</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:restriction base="tSoleProprietorData">
        <xsd:sequence>
          <xsd:element name="name" type="string-1000">
            <xsd:annotation>
              <xsd:documentation xml:lang="ru">Наименование</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="inn" type="smev:PhysicalPersonINNType">
            <xsd:annotation>
              <xsd:documentation xml:lang="ru">ИНН</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="ogrn" type="smev:OGRNSoleProprietorType">
            <xsd:annotation>
              <xsd:documentation xml:lang="ru">ОГРН</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:restriction>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="tLegalPersonIdCodesOnly">
    <xsd:annotation>
      <xsd:documentation xml:lang="ru">Тип юридического лица только с идентификационными кодами</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:restriction base="tLegalPersonData">
        <xsd:sequence>
          <xsd:element name="inn" type="smev:LegalPersonINNType">
            <xsd:annotation>
              <xsd:documentation xml:lang="ru">ИНН</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="kpp" type="smev:KPPType" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation xml:lang="ru">КПП</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="ogrn" type="smev:OGRNCompanyType">
            <xsd:annotation>
              <xsd:documentation xml:lang="ru">ОГРН</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:restriction>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="tInfoProvider">
    <xsd:annotation>
      <xsd:documentation xml:lang="ru">Тип поставщика информации</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="tLegalPerson">
        <xsd:sequence>
          <xsd:element name="codeParticipant" type="tParticipantCode">
            <xsd:annotation>
              <xsd:documentation xml:lang="ru">Код участника ЕГИССО</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <!--========================================== Типы, используемые в данных юридического лица  ==========================================-->
  <xsd:complexType name="tLegalPersonData">
    <xsd:annotation>
      <xsd:documentation xml:lang="ru">Данные, используемые в юридическом лице</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="name" type="string-1000" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation xml:lang="ru">Наименование</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="inn" type="smev:LegalPersonINNType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation xml:lang="ru">ИНН</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="kpp" type="smev:KPPType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation xml:lang="ru">КПП</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ogrn" type="smev:OGRNCompanyType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation xml:lang="ru">ОГРН</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="tSoleProprietorData">
    <xsd:annotation>
      <xsd:documentation xml:lang="ru">Данные, используемые для индивидуального предпринимателя (ИП)</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="name" type="string-1000" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation xml:lang="ru">Наименование</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="inn" type="smev:PhysicalPersonINNType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation xml:lang="ru">ИНН</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ogrn" type="smev:OGRNSoleProprietorType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation xml:lang="ru">ОГРН</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <!--==========================================  Адресные типы  ==========================================-->
  <xsd:complexType name="tAddress">
    <xsd:annotation>
      <xsd:documentation xml:lang="ru">Выбор вида адреса</xsd:documentation>
    </xsd:annotation>
    <xsd:choice>
      <xsd:element name="russianAddressNotResticted" type="tRussianAddressNotRestricted">
        <xsd:annotation>
          <xsd:documentation xml:lang="ru">Российский адрес без ограничения заполнения элементов адреса</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="foreignAddress" type="tForeignAddress">
        <xsd:annotation>
          <xsd:documentation xml:lang="ru">Иностранный адрес</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:choice>
  </xsd:complexType>
  <xsd:complexType name="tRussianAddressNotRestricted">
    <xsd:annotation>
      <xsd:documentation xml:lang="ru">Российский адрес без ограничения заполнения элементов адреса</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:restriction base="tRussianAddressData">
        <xsd:sequence>
          <xsd:element name="garAddressObjectGuid" type="tUuid" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation xml:lang="ru">Глобальный уникальный идентификационный код адресного объекта в Государственном адресном реестре (ФИАС)</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="postIndex" type="tRussianPostIndex" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation xml:lang="ru">Почтовый индекс</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="fullAddress" type="string-2000" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation xml:lang="ru">Полный адрес строкой</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="structuredElements" type="tAddressStructuredElements" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation xml:lang="ru">Структурированные элементы адреса</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="oktmoCode" type="tOktmoCode" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation xml:lang="ru">Код ОКТМО</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:restriction>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="tRegistrationAndFactAddresses">
    <xsd:annotation>
      <xsd:documentation>Тип адресов регистрации по месту жительства и пребывания, фактического адреса пребывания</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="residenceRegistrationAddress" type="tAddress" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Адрес регистрации по месту жительства</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="temporaryRegistrationAddress" type="tAddress" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Адрес регистрации по месту пребывания</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="factAddress" type="tAddress" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Адрес фактического проживания или пребывания</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="tLegalAndFactAddresses">
    <xsd:annotation>
      <xsd:documentation>Тип юридического и фактического адресов юридических лиц</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="legalAddress" type="tAddress">
        <xsd:annotation>
          <xsd:documentation>Юридический адрес</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="factAddress" type="tAddress">
        <xsd:annotation>
          <xsd:documentation>Фактический адрес</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="tForeignAddress">
    <xsd:annotation>
      <xsd:documentation xml:lang="ru">Иностранный адрес</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="postIndex" type="string-10" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation xml:lang="ru">Почтовый индекс (код)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="fullAddress" type="string-2000" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation xml:lang="ru">Полный адрес строкой</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="countryCode" type="tOksmCode">
        <xsd:annotation>
          <xsd:documentation xml:lang="ru">Код страны по ОКСМ</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="address" type="tAddressStructuredElements">
        <xsd:annotation>
          <xsd:documentation xml:lang="ru">Структурированные элементы адреса</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="tAddressWithType">
    <xsd:annotation>
      <xsd:documentation xml:lang="ru">Выбор вида адреса с указанием кода типа адреса в ЕГИССО</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="tAddress">
        <xsd:sequence>
          <xsd:element name="addressTypeCode" type="tAddressTypeCode">
            <xsd:annotation>
              <xsd:documentation xml:lang="ru">Код типа адреса в ЕГИССО</xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <!--==========================================  Типы, используемые в адресных данных  ==========================================-->
  <xsd:complexType name="tRussianAddressData">
    <xsd:annotation>
      <xsd:documentation xml:lang="ru">Данные, используемые в российском адресе</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="garAddressObjectGuid" type="tUuid" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation xml:lang="ru">Глобальный уникальный идентификационный код адресного объекта в Государственном адресном реестре (ФИАС)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="postIndex" type="tRussianPostIndex" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation xml:lang="ru">Почтовый индекс</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="fullAddress" type="string-2000" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation xml:lang="ru">Полный адрес строкой</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="structuredElements" type="tAddressStructuredElements" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation xml:lang="ru">Структурированные элементы адреса</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="oktmoCode" type="tOktmoCode" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation xml:lang="ru">Код ОКТМО</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="tAddressStructuredElements">
    <xsd:annotation>
      <xsd:documentation xml:lang="ru">Структурированные элементы адреса</xsd:documentation>
