      </xsd:element>
      <xsd:element name="codeType" type="tDocumentTypeCode" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation xml:lang="ru">Код вида документа</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="name" type="string-500" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation xml:lang="ru">Наименование вида документа</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="effectiveDate" type="tDate" minOccurs="0">
