<?xml version="1.0" encoding="utf-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:smev="urn://x-artefacts-smev-gov-ru/supplementary/commons/1.0.4"	xmlns="urn://egisso-ru/types/basic/2.0.0" targetNamespace="urn://egisso-ru/types/basic/2.0.0" elementFormDefault="qualified" attributeFormDefault="unqualified"	version="1.1">
	<xsd:import namespace="urn://x-artefacts-smev-gov-ru/supplementary/commons/1.0.4" schemaLocation="../smev3/smev-supplementary-commons-1.0.4.xsd"/>
	<!--========================================== Типы данных физического лица  ==========================================-->
	<xsd:complexType name="tPhysicalPersonWithoutBirthPlace">
		<xsd:annotation>
			<xsd:documentation>Тип физического лица со всеми обязательными данными кроме места рождения</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:restriction base="tPhysicalPersonData">
				<xsd:sequence>
					<xsd:element name="snils" type="smev:SNILSType">
						<xsd:annotation>
							<xsd:documentation>Страховой номер индивидуального лицевого счёта</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="fio" type="tFio">
						<xsd:annotation>
							<xsd:documentation>ФИО</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="gender" type="tGender">
						<xsd:annotation>
							<xsd:documentation>Пол</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="birthDate" type="xsd:date">
						<xsd:annotation>
							<xsd:documentation>Дата рождения</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="birthPlace" type="tAddress" minOccurs="0" maxOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Место рождения</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="citizenshipCode" type="tOksmCode">
						<xsd:annotation>
							<xsd:documentation>Код страны гражданства</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:restriction>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="tPhysicalPersonSnilsOnly">
		<xsd:annotation>
			<xsd:documentation>Тип физического лица только со СНИЛС</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:restriction base="tPhysicalPersonData">
				<xsd:sequence>
					<xsd:element name="snils" type="smev:SNILSType">
						<xsd:annotation>
							<xsd:documentation>Страховой номер индивидуального лицевого счёта</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="fio" type="tFio" minOccurs="0" maxOccurs="0">
						<xsd:annotation>
							<xsd:documentation>ФИО</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="gender" type="tGender" minOccurs="0" maxOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Пол</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="birthDate" type="xsd:date" minOccurs="0" maxOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Дата рождения</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="birthPlace" type="tAddress" minOccurs="0" maxOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Место рождения</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="citizenshipCode" type="tOksmCode" minOccurs="0" maxOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Код страны гражданства</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:restriction>
		</xsd:complexContent>
	</xsd:complexType>
	<!--========================================== Типы, используемые в данных физического лица  ==========================================-->
	<xsd:complexType name="tPhysicalPersonData">
		<xsd:annotation>
			<xsd:documentation>Данные, используемые в типах физического лица</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="snils" type="smev:SNILSType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Страховой номер индивидуального лицевого счёта</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="fio" type="tFio" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>ФИО</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="gender" type="tGender" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Пол</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="birthDate" type="xsd:date" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дата рождения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="birthPlace" type="tAddress" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Место рождения</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="citizenshipCode" type="tOksmCode" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Код страны гражданства</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="tFio">
		<xsd:annotation>
			<xsd:documentation>Фамилия, имя и отчество</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="surname" type="smev:string-100">
				<xsd:annotation>
					<xsd:documentation>Фамилия</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="patronymic" type="smev:string-100" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Отчество</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="forename" type="smev:string-100">
				<xsd:annotation>
					<xsd:documentation>Имя</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:simpleType name="tGender">
		<xsd:annotation>
			<xsd:documentation>Пол</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="string-1">
			<xsd:pattern value="[MFU]"/>
			<xsd:enumeration value="M">
				<xsd:annotation>
					<xsd:documentation>Мужской пол</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="F">
				<xsd:annotation>
					<xsd:documentation>Женский пол</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="U">
				<xsd:annotation>
					<xsd:documentation>Пол неопределен</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="tSnils-9">
		<xsd:annotation>
			<xsd:documentation xml:lang="ru">СНИЛС (9-ти разрядный) без контрольной суммы</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:length value="9"/>
			<xsd:pattern value="\d{9}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!--========================================== Типы данных юридического лица  ==========================================-->
	<xsd:complexType name="tLegalPerson">
		<xsd:annotation>
			<xsd:documentation>Тип юридического лица со всеми обязательными данными</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:restriction base="tLegalPersonData">
				<xsd:sequence>
					<xsd:element name="name" type="string-1000">
						<xsd:annotation>
							<xsd:documentation>Наименование</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="inn" type="smev:LegalPersonINNType">
						<xsd:annotation>
							<xsd:documentation>ИНН</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="ogrn" type="smev:OGRNCompanyType">
						<xsd:annotation>
							<xsd:documentation>ОГРН</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:restriction>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="tLegalPersonIdCodesOnly">
		<xsd:annotation>
			<xsd:documentation>Тип юридического лица только с идентификационными кодами</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:restriction base="tLegalPersonData">
				<xsd:sequence>
					<xsd:element name="name" type="string-1000" minOccurs="0" maxOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Наименование</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="inn" type="smev:LegalPersonINNType">
						<xsd:annotation>
							<xsd:documentation>ИНН</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="ogrn" type="smev:OGRNCompanyType">
						<xsd:annotation>
							<xsd:documentation>ОГРН</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:restriction>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="tInfoProvider">
		<xsd:annotation>
			<xsd:documentation>Тип поставщика информации</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="tLegalPerson">
				<xsd:sequence>
					<xsd:element name="codeParticipant" type="tParticipantCode">
						<xsd:annotation>
							<xsd:documentation>Код участника ЕГИССО</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<!--========================================== Типы, используемые в данных юридического лица  ==========================================-->
	<xsd:complexType name="tLegalPersonData">
		<xsd:annotation>
			<xsd:documentation>Данные, используемые в юридическом лице</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="name" type="string-1000" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Наименование</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="inn" type="smev:LegalPersonINNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>ИНН</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ogrn" type="smev:OGRNCompanyType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>ОГРН</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!--==========================================  Адресные типы  ==========================================-->
	<xsd:complexType name="tAddress">
		<xsd:annotation>
			<xsd:documentation>Выбор вида адреса</xsd:documentation>
		</xsd:annotation>
		<xsd:choice>
			<xsd:element name="russianAddressNotResticted" type="tRussianAddressNotRestricted">
				<xsd:annotation>
					<xsd:documentation>Российский адрес без ограничения заполнения элементов адреса</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="russianAddresFias" type="tRussianAddressStrictFias">
				<xsd:annotation>
					<xsd:documentation>Российский адрес с обязательным указанием guid ФИАС</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="russianAddressOneString" type="tRussianAddressOneString">
				<xsd:annotation>
					<xsd:documentation>Российский адрес одной строкой текстом</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="foreignAddress" type="tForeignAddress">
				<xsd:annotation>
					<xsd:documentation>Иностранный адрес</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:choice>
	</xsd:complexType>
	<xsd:complexType name="tRussianAddressNotRestricted">
		<xsd:annotation>
			<xsd:documentation>Российский адрес без ограничения заполнения элементов адреса</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:restriction base="tRussianAddressData">
				<xsd:sequence>
					<xsd:element name="guidFias" type="tUuid" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Глобальный уникальный идентификационный код адресного объекта ФИАС</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="postIndex" type="tRussionPostIndex" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Почтовый индекс</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="fullAddress" type="string-2000" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Полный адрес строкой</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="structuredElements" type="tRussianAddressStructuredElements" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Структурированные элементы адреса</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="addressTypeCode" type="tAddressTypeCode"/>
				</xsd:sequence>
			</xsd:restriction>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="tRussianAddressStrictFias">
		<xsd:annotation>
			<xsd:documentation>Российский адрес с обязательным указанием guid ФИАС</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:restriction base="tRussianAddressData">
				<xsd:sequence>
					<xsd:element name="guidFias" type="tUuid">
						<xsd:annotation>
							<xsd:documentation>Глобальный уникальный идентификационный код адресного объекта ФИАС</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="addressTypeCode" type="tAddressTypeCode"/>
				</xsd:sequence>
			</xsd:restriction>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="tRussianAddressOneString">
		<xsd:annotation>
			<xsd:documentation>Российский адрес одной строкой текстом</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:restriction base="tRussianAddressData">
				<xsd:sequence>
					<xsd:element name="guidFias" type="tUuid" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Глобальный уникальный идентификационный код адресного объекта ФИАС</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="postIndex" type="tRussionPostIndex" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Почтовый индекс</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="fullAddress" type="string-2000">
						<xsd:annotation>
							<xsd:documentation>Полный адрес строкой</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="structuredElements" type="tRussianAddressStructuredElements" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Структурированные элементы адреса</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="addressTypeCode" type="tAddressTypeCode"/>
				</xsd:sequence>
			</xsd:restriction>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="tForeignAddress">
		<xsd:annotation>
			<xsd:documentation>Иностранный адрес</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="postIndex" type="smev:string-10" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Почтовый индекс (код)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="countryCode" type="tOksmCode">
				<xsd:annotation>
					<xsd:documentation>Код страны</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="countryName" type="string-256" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Название страны</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="fullAddress" type="string-2000">
				<xsd:annotation>
					<xsd:documentation>Полный адрес строкой</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="addressTypeCode" type="tAddressTypeCode"/>
		</xsd:sequence>
	</xsd:complexType>
	<!--==========================================  Типы, используемые в адресных данных  ==========================================-->
	<xsd:complexType name="tRussianAddressData">
		<xsd:annotation>
			<xsd:documentation>Данные, используемые в российском адресе</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="guidFias" type="tUuid" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Глобальный уникальный идентификационный код адресного объекта ФИАС</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="postIndex" type="tRussionPostIndex" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Почтовый индекс</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="fullAddress" type="string-2000" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Полный адрес строкой</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="structuredElements" type="tRussianAddressStructuredElements" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Структурированные элементы адреса</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="addressTypeCode" type="tAddressTypeCode" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="tRussianAddressStructuredElements">
		<xsd:annotation>
			<xsd:documentation>Структурированные элементы российского адреса</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="region" type="string-256" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Регион</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="district" type="string-256" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Район</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="city" type="string-256" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Наименование города</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="cityArea" type="string-256" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Район города</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="locality" type="string-256" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Населенный пункт</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="street" type="string-256" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Улица</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="house" type="string-256" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Дом</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="building" type="smev:string-10" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Блок, здание, корпус</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="flat" type="smev:string-10" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Квартира</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:simpleType name="tRussionPostIndex">
		<xsd:annotation>
			<xsd:documentation>Российский почтовый индекс</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="\d{6}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="tAddressTypeCode">
		<xsd:annotation>
			<xsd:documentation>Код типа адреса в ЕГИССО</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:length value="2"/>
			<xsd:pattern value="\d{2}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="tOktmoCode11">
		<xsd:annotation>
			<xsd:documentation>Код ОКТМО, включающий муниципальные образования и населенные пункты (11-разрядный, цифровой)</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="\d{11}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="tOktmoCode8">
		<xsd:annotation>
			<xsd:documentation>Код ОКТМО, включающий только муниципальные образования (8-разрядный, цифровой)</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="\d{8}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!--==========================================  Типы документов  ==========================================-->
	<xsd:complexType name="tDefinedDocument">
		<xsd:annotation>
			<xsd:documentation>Идентифицированный вид документа</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:restriction base="tDocumentData">
				<xsd:sequence>
					<xsd:element name="date" type="tDocumentDate">
						<xsd:annotation>
							<xsd:documentation>Дата документа</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="number" type="smev:string-50" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Номер документа</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="codeType" type="tDocumentTypeCode">
						<xsd:annotation>
							<xsd:documentation>Код вида документа</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="name" type="smev:string-500" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Наименование вида документа</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="effectiveDate" type="xsd:date" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Дата вступления документа в силу</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="validityEndDate" type="xsd:date" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Дата окончания периода действия документа</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="series" type="smev:string-20" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Серия документа</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="issuer" type="smev:string-500">
						<xsd:annotation>
							<xsd:documentation>Наименование организации (органа) выпустившего документ</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:restriction>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="tDefinedDocumentWithEffectiveDate">
		<xsd:annotation>
			<xsd:documentation>Идентифицированный вид документа с датой вступления в силу</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:restriction base="tDefinedDocument">
				<xsd:sequence>
					<xsd:element name="date" type="tDocumentDate">
						<xsd:annotation>
							<xsd:documentation>Дата документа</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="number" type="smev:string-50" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Номер документа</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="codeType" type="tDocumentTypeCode">
						<xsd:annotation>
							<xsd:documentation>Код вида документа</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="name" type="smev:string-500" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Наименование вида документа</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="effectiveDate" type="xsd:date">
						<xsd:annotation>
							<xsd:documentation>Дата вступления документа в силу</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="validityEndDate" type="xsd:date" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Дата окончания периода действия документа</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="series" type="smev:string-20" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Серия документа</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="issuer" type="smev:string-500">
						<xsd:annotation>
							<xsd:documentation>Наименование организации (органа) выпустившего документ</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:restriction>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="tUndefinedDocument">
		<xsd:annotation>
			<xsd:documentation>Неидентифицированный вид документа</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:restriction base="tDefinedDocument">
				<xsd:sequence>
					<xsd:element name="date" type="tDocumentDate">
						<xsd:annotation>
							<xsd:documentation>Дата документа</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="number" type="smev:string-50" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Номер документа</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="codeType" type="tDocumentTypeCode" fixed="00044">
						<xsd:annotation>
							<xsd:documentation>Код вида документа</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="name" type="smev:string-500">
						<xsd:annotation>
							<xsd:documentation>Наименование вида документа</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="effectiveDate" type="xsd:date" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Дата вступления документа в силу</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="validityEndDate" type="xsd:date" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Дата окончания периода действия документа</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="series" type="smev:string-20" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Серия документа</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="issuer" type="smev:string-500">
						<xsd:annotation>
							<xsd:documentation>Наименование организации (органа) выпустившего документ</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:restriction>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="tUndefinedDocumentWithEffectiveDate">
		<xsd:annotation>
			<xsd:documentation>Неидентифицированный вид документа с датой вступления в силу</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:restriction base="tUndefinedDocument">
				<xsd:sequence>
					<xsd:element name="date" type="tDocumentDate">
						<xsd:annotation>
							<xsd:documentation>Дата документа</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="number" type="smev:string-50" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Номер документа</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="codeType" type="tDocumentTypeCode" fixed="00044">
						<xsd:annotation>
							<xsd:documentation>Код вида документа</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="name" type="smev:string-500">
						<xsd:annotation>
							<xsd:documentation>Наименование вида документа</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="effectiveDate" type="xsd:date">
						<xsd:annotation>
							<xsd:documentation>Дата вступления документа в силу</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="validityEndDate" type="xsd:date" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Дата окончания периода действия документа</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="series" type="smev:string-20" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Серия документа</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="issuer" type="smev:string-500">
						<xsd:annotation>
							<xsd:documentation>Наименование организации (органа) выпустившего документ</xsd:documentation>
						</xsd:annotation>
